{
	"expo": {
		"name": "PhotoBrain",
		"slug": "photobrain",
		"version": "0.1.0",
		"scheme": "photobrain",
		"orientation": "portrait",
		"userInterfaceStyle": "automatic",
		"assetBundlePatterns": ["**/*"],
		"ios": {
			"supportsTablet": true,
			"bundleIdentifier": "com.photobrain.app"
		},
		"android": {
			"adaptiveIcon": {
				"backgroundColor": "#ffffff"
			},
			"package": "com.photobrain.app"
		},
		"web": {
			"bundler": "metro",
			"output": "single"
		},
		"plugins": ["expo-router"]
	}
}
